<head>
    <link rel='stylesheet' href='/stylesheets/table.css' />

</head>


{{#if body}}
<table border="1">
     <tr><th>Id</th> <th>Record</th> <th>Vessel Terms</th> <th>Feature type</th> <th>Chart</th> <th>Lattitude</th> 
     <th>Longitude</th> <th>GP Quality</th> <th>Depth</th> <th>Sounding type</th><th>History</th> <th>Quasou</th> <th>Water Level</th><th>Co ordinates</th></tr>
    {{#each body}}
    <tr>
        <td>{{this._id}}</td>
        <td>{{this.recrd}}</td>
        <td>{{this.vesslterms}}</td>        
        <td>{{this.feature_type}}</td>
        <td>{{this.chart}}</td>
        <td>{{this.latdec}}</td>
        <td>{{this.londec}}</td>
        <td>{{this.gp_quality}}</td>
        <td>{{this.depth}}</td>
        <td>{{this.sounding_type}}</td>
        <td>{{this.history}}</td>
        <td>{{this.quasou}}</td>
        <td>{{this.watlev}}</td>
        <td>{{this.coordinates}}</td>

        

    </tr>
    {{/each}}
</table>
{{/if}}
